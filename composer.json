{
    "name": "symfony-cmf/menu-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Menu Bundle",
    "keywords": ["Symfony CMF", "menu"],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/MenuBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.5.9|^7.0",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony-cmf/core-bundle": "^2.0@dev",
        "doctrine/phpcr-bundle": "^1.1",
        "doctrine/phpcr-odm": "^2.0",
        "knplabs/knp-menu-bundle": "^2.0.0",
        "knplabs/knp-menu": "^2.0.0"
    },
    "require-dev": {
        "symfony/monolog-bundle": "^2.3|^3.0",
        "symfony-cmf/routing-bundle": "^1.4|^2.0",
        "symfony-cmf/testing": "^2.0",
        "twig/twig": "^1.18|^2.0",
        "sonata-project/doctrine-phpcr-admin-bundle": "^2.0",
        "sonata-project/admin-bundle": "^2.3.7|^3.0",
        "sonata-project/block-bundle": "^2.2.8|^3.0",
        "sonata-project/core-bundle": "^2.2.5|^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "sonata-project/doctrine-phpcr-admin-bundle": "if you want to have editing capabilities (^1.1)",
        "burgov/key-value-form-bundle": "if you want to edit the advanced options on the node with the menu options extension"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\MenuBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
